# Mobile App Environment Configuration
# Copy this file to .env and update the values

# Environment (development, staging, production)
EXPO_PUBLIC_ENV=production

# API URL (Point this to your domain with HTTPS)
EXPO_PUBLIC_API_URL=https://goexec.de/api/v1

# WebSocket URL (Point this to your domain with WSS for secure websocket)
EXPO_PUBLIC_WS_URL=wss://goexec.de/api/v1/mobile/ws

# Logging
EXPO_PUBLIC_ENABLE_LOGGING=true
